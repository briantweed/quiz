{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@components/*": ["app/components/*"],
            "@layouts/*": ["app/components/layouts/*"],
            "@content/*": ["app/components/page-content/*"],
            "@libraries/*": ["app/components/libraries/*"],
            "@store": ["app/store/store"],
            "@constants": ["app/constants/"],
            "@api/*": ["app/hooks/api/*"]
        }
    },
    "exclude": ["node_modules"]
}
